{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"custom": {
			"w" : {
				"full": "100vw",
				"half": "calc(var(--wp--style--global--content-size) / 2)"
			},
			"img" : {
				"grayscale": "contrast(1.1) grayscale(1)"
			},
			"accordion" : {
				"background": "linear-gradient(0deg, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.2)), #4698CB",
				"borderRadius": "8px",
				"fontSize": "18px",
				"lineHeight": "29px"
			}
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [
				{
					"slug": "gradient-blue",
					"gradient": "linear-gradient(99.77deg, #AFD8F1 33.13%, #4698CB 57.34%)",
					"name": "Gradient Blue"
				}
			],
			"link": false,
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#4698CB"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#D50000"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#AFD8F1"
				},
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "Gray",
					"slug": "gray",
					"color": "#0E0E0E"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#FFFFFF"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "1024px",
			"wideSize": "1200px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [
				"px",
				"rem"
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": false,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Montserrat Alternates\", sans-serif",
					"name": "Montserrat Alternates",
					"slug": "montserrat-alternates",
					"fontFace": [
						{
							"fontFamily": "Montserrat Alternates Regular",
							"fontStyle": "normal",
							"fontWeight": "normal",
							"src": [
								"file:./src/fonts/MontserratAlternates-Regular.ttf"
							]
						},
						{
							"fontFamily": "Montserrat Alternates Italic",
							"fontStyle": "italic",
							"fontWeight": "normal",
							"src": [
								"file:./src/fonts/MontserratAlternates-Italic.ttf"
							]
						},
						{
							"fontFamily": "Montserrat Alternates Bold",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./src/fonts/MontserratAlternates-Bold.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "\"Eveleth Clean\"",
					"name": "Eveleth Clean",
					"slug": "eveleth-clean",
					"fontFace": [
						{
							"fontFamily": "Eveleth Clean",
							"fontStyle": "normal",
							"fontWeight": "normal",
							"fontDisplay": "swap",
							"src": [
								"file:./src/fonts/EvelethCleanRegular.woff"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "size-14",
					"size": "14px",
					"name": "Tiny"
				},
				{
					"slug": "size-16",
					"size": "16px",
					"name": "Small"
				},
				{
					"slug": "size-18",
					"size": "18px",
					"name": "Normal"
				},
				{
					"slug": "size-20",
					"size": "18px",
					"name": "Medium",
					"fluid": {
						"min": "18px",
						"max": "20px"
					}
				},
				{
					"slug": "size-max-36",
					"size": "24px",
					"name": "Large",
					"fluid": {
						"min": "24px",
						"max": "36px"
					}
				},
				{
					"slug": "size-max-60",
					"size": "30px",
					"name": "Extra Large",
					"fluid": {
						"min": "30px",
						"max": "60px"
					}
				}
			],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": true,
			"textTransform": true
		},
		"blocks": {
			"core/paragraph": {},
			"core/button": {}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--gray)",
			"text": "var(--wp--preset--color--tertiary)"
		},
		"spacing": {
			"blockGap": "32px"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily": "var(--wp--preset--font-family--montserrat-alternates)",
			"fontWeight": "400",
			"lineHeight": "1.5"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eveleth-clean)",
					"fontSize": "var(--wp--preset--font-size--size-max-60)",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eveleth-clean)",
					"fontSize": "var(--wp--preset--font-size--size-max-36)",
					"lineHeight": "1.2"
				}
			},
			"link":{
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "#ffb383"
					}
				}
			}
		},
		"blocks": {
			"core/heading": {},
			"core/paragraph": {
				"elements": {
					"link": {
						"color": {
								"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--size-18)"
				}
			},
			"core/list": {}
		},
		"css": ".c-accordion__title { background: var(--wp--custom--accordion--background); border-radius: var(--wp--custom--accordion--border-radius); font-size: var(--wp--custom--accordion--font-size); color: var(--wp--preset--color--white); padding: 28px 34px;} .wp-site-blocks > .wp-block-group { margin-block-start: 0;} header.floating.header-no-nav.wp-block-template-part {position: fixed; top: 25px; left: 50%; transform: translate(-50%);z-index: 100;} .home .home-faq > .wp-block-columns { gap: 0 40px; } .home .wp-site-blocks > .wp-block-cover {margin-block-start: 0;}"
	},
	"customTemplates": [],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}